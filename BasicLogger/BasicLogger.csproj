<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <RootNamespace>Peamel.BasicLogger</RootNamespace>
    <AssemblyName>PeamelBasicLogger</AssemblyName>
    <Version>1.0.2</Version>
    <Authors>Dave Brooks</Authors>
    <Company />
    <Product>BasicLogger</Product>
    <Description>A basic netstandard logger</Description>
    <PackageProjectUrl>https://github.com/brooksyott/BasicLogger</PackageProjectUrl>
    <AssemblyVersion>1.0.2.0</AssemblyVersion>
    <FileVersion>1.0.2.0</FileVersion>
    <PackageReleaseNotes>Fixed bug #1. Multiple locks in rotate log file.</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>netstandard, logger, peamel, xamarin forms</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Threading.Thread" Version="4.3.0" />
  </ItemGroup>

</Project>
